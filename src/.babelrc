{
	"presets": [
		["es2015", {"modules": false}],
		"stage-0",
		"react"
	],
	"plugins": [
		"transform-flow-strip-types",
		"transform-decorators-legacy",
		"dev-expression",
		"syntax-dynamic-import"
	],
	"env": {
		"development": {
			"plugins": [
				"react-hot-loader/babel"
			]
		},
		"test": {
			"plugins": [
				"transform-es2015-modules-commonjs",
				"dynamic-import-node",
				["module-resolver", {
					"root": ["./src"]
				}]
			]
		}
	}
}
